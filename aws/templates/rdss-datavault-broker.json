[
  {
    "command": [
      "consumer"
    ],
    "cpu": 256,
    "environment": [
      {
        "name": "MYSQL_PASSWORD",
        "value": "abcd1234"
      },
      {
        "name": "RABBITMQ_PASSWORD",
        "value": "efgh5678"
      },
      {
        "name": "AWS_S3_BUCKET",
        "value": "${archive_bucket_name}"
      }
    ],
    "essential": true,
    "image": "${image_url}",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${log_group_region}"
      }
    },
    "memory": 512,
    "name": "${container_name}"
  }
]
